              <td>0</td>
              <td>0</td>
              <td>0</td>
              <td>0</td>
              <td>false</td>
            </tr>
            <tr>
              <td>000000002</td>
              <td>Jane</td>
              <td>jane@email.com</td>
              <td>Doe</td>
              <td>Internal</td>
              <td>Research and Technology</td>
              <td>10</td>
              <td>4</td>
              <td>4</td>
              <td>2</td>
              <td>true</td>
            </tr>
            <tr>
              <td>000000003</td>
              <td>Jackson</td>
              <td>jackson@email.com</td>
              <td>Doe</td>
              <td>Internal</td>
              <td>Community Building</td>
